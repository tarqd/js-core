{
  "name": "react-ssr-example",
  "version": "1.0.0",
  "type": "module",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "source": "src/client/index.html",
  "exports": {
    "shared/ldclient": {
      "browser": "./src/shared/ldclient.browser.js",
      "default": "./src/shared/ldclient.js"
    }
  },
  "alias": {
    "react/jsx-runtime": "react/jsx-runtime.js"
},
  "targets": {
    "browser": {
      "source": "src/client/index.html",
      "distDir": "dist/client",
      "context": "browser",
      "publicUrl": ".",
      "isLibrary": false,
      "includeNodeModules": true,
      "engines": {
        "browsers": "Chrome 100"
      }
      
      
    },
    "backend": {
      "source": "src/server/index.js",
      "distDir": "dist/server",
      "isLibrary": true,
      "includeNodeModules": false,
      "outputFormat": "esmodule",
      "context": "node"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@launchdarkly/node-server-sdk": "^9.0.4",
    "express": "^4.18.2",
    "launchdarkly-js-client-sdk": "^3.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.10.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "util": "^0.12.5"
  }
}
